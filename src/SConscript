Import('env')

import os

# List of all source files
sourceFiles = ['solver/main.cpp', 'solver/FCore.cpp']

# Add source files to scons env
for f in sourceFiles:
    env.srcFiles.append(env.Object(f))

# Declare cxx test suites
env.CxxTest('FDummyTestSuite', ['unit_tests/FDummyTests.t.h', 'solver/FDummy.cpp'])

Export('env')